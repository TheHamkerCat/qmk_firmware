{
    "manufacturer": "QMK",
    "maintainer": "adeveloper79",
    "keyboard_name": "8Key Encoder",
    "development_board": "blackpill_f411",
    "bootloader": "tinyuf2",
    "usb": {
        "vid": "0x7378",
        "pid": "0x1278",
        "device_version": "0.1.0",
        "shared_endpoint": {
            "keyboard": true
        }
    },
    "diode_direction": "COL2ROW",
    "features": {
        "bootmagic": false,
        "mousekey": false,
        "extrakey": true,
        "console": true,
        "command": false,
        "nkro": false
    },
    "qmk": {
        "locking": {
            "enabled": true,
            "resync": true
        }
    },
    "matrix_pins": {
        "direct": [
            ["A7", "A6", "A5", "A4", "B10"],  //B10 as Encoder Switch
            ["A3", "A2", "A1", "A0"]
        ]
    },
    "layouts": {
        "LAYOUT": {
            "layout": [
                {"matrix": [0, 0], "x": 0, "y": 0},
                {"matrix": [0, 1], "x": 0, "y": 1},
                {"matrix": [0, 2], "x": 0, "y": 2},
                {"matrix": [0, 3], "x": 0, "y": 3},
                {"matrix": [0, 4], "x": 0, "y": 4},

                {"matrix": [1, 0], "x": 1, "y": 0},
                {"matrix": [1, 1], "x": 1, "y": 1},
                {"matrix": [1, 2], "x": 1, "y": 2},
                {"matrix": [1, 3], "x": 1, "y": 3}
            ]
        }
    }
}
